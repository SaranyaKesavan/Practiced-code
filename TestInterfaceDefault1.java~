interface Drawable
{
	void draw();
	private void msg()
	{
		System.out.println("default method");
	}
}
class Rectangle implements Drawable
{
	public void draw()
	{
		System.out.println("drawing rectangle");
	}
	 void msg()
	{
		System.out.println("default method");
	}
}
class TestInterfaceDefault1
{
	public static void main(String args[])
	{
		Drawable d=new Rectangle();
		d.draw();
		d.msg();
	}
}

import java.util.*;
class StringComparison
{
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		String string1,string2;
		System.out.println("Enter the first string:");
		string1=sc.nextLine();
		System.out.println("Enter the second string:");
		string2=sc.nextLine();
		StringComparison s=new StringComparison();
		s.compare(string1,string2);
	}
	void compare(String string1, String string2)
	{
		System.out.println("string comparison using equals() : "+string1.equals(string2));
		System.out.print("string comparison using == : ");
		System.out.println(	string1==string2);
		System.out.println("string comparison using compareTo() : "+string1.compareTo(string2));
	}
}
